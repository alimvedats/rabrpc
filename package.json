{
  "name": "rabrpc",
  "version": "0.1.3",
  "description": "RabbitMQ RPC Library based on Rabbot",
  "main": "index.js",
  "scripts": {
    "preversion": "npm test",
    "test": "snyk test && eslint . && npm run test-unit && npm run test-integration",
    "test-unit": "mocha \"test/bootstrap.test.js\" \"test/unit/**/*.test.js\"",
    "test-integration": "mocha \"test/bootstrap.test.js\" \"test/integration/**/*.test.js\"",
    "coverage": "istanbul cover _mocha \"test/bootstrap.test.js\" \"test/unit/**/*.test.js\""
  },
  "keywords": [
    "rabbitmq",
    "microservice",
    "pubsub",
    "reqres",
    "rpc"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:swarthy/rabrpc.git"
  },
  "author": "Alexander Mochalin <horroshow@mail.ru>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.6",
    "jsend": "^1.0.2",
    "lodash": "^4.16.0",
    "rabbot": "^1.0.6",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.5.0",
    "eslint-config-inclusive": "^1.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.19.1"
  }
